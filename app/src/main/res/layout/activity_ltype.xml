                    android:layout_height="50dp"
                    style="@style/Icon"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/kinesthetic_style" />

            </LinearLayout>

        </LinearLayout>


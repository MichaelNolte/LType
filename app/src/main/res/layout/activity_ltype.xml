            android:layout_height="match_parent"
            android:text="@string/app_title_slogan"
            android:textSize="16sp"
            android:textAlignment="center"
            android:fontFamily="sans-serif-black"
            android:paddingBottom="@dimen/standard_padding"/>

        <Button

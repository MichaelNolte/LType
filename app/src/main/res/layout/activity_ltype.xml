            android:layout_height="wrap_content"
            android:onClick="popUpAppDescription"
            android:text="@string/button_app_description"/>

        <TextView
            android:id="@+id/app_description_text_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/app_description"
            style="@style/PopUp"/>

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="popUpLearningTypeInfo"
            android:text="@string/button_learning_styles"/>

